{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "ignoreCommand": "git diff HEAD^ HEAD ./src/ || exit 0",
    "functions": {
        "api/**/*": {
            "maxDuration": 10
        }
    }
}