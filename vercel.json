{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "ignoreCommand": "git diff --quiet HEAD^ HEAD ./api/",
    "functions": {
        "api/**/*": {
            "maxDuration": 0 
        }
    }
}

